    627          	//fcf= pConfig->ackRequest ? BASIC_RF_FCF_ACK : BASIC_RF_FCF_NOACK;
    628          	fcf = pConfig->frameHead;
    629          	pHdr->fcf0 = LO_UINT16(fcf);
    630          	pHdr->fcf1 = HI_UINT16(fcf);
    631          	pHdr->seqNumber= txState.txSeqNumber;
    632          	pHdr->panId= 0xFFFF;
    633              	osal_memcpy(pHdr->extAddr,extAddr,8);
