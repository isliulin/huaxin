    615          static uint8 LongAddrBuildHeader(uint8* buffer, uint8 *extAddr, uint8 payloadLength, uint8 sendMod)
    616          {
    617          	extRfPktHdr_t *pHdr;
    618          	uint16 fcf;
    619          
    620          	pHdr= (extRfPktHdr_t*)buffer;
    621          
