    283           * @fn      MT_UartProcessZAppData
    284           *
    285           * @brief   | SOP | CMD  |   Data Length   | FSC  |
    286           *          |  1  |  2   |       1         |  1   |
    287           *
    288           *          Parses the data and determine either is SPI or just simply serial data
    289           *          then send the data to correct place (MT or APP)
    290           *
    291           * @param   port    - UART port
    292           *          event   - Event that causes the callback
    293           *
    294           *
    295           * @return  None
    296           ***************************************************************************************************/
    297          void MT_UartProcessZAppData ( uint8 port, uint8 event )
    298          {
    299          
    300            osal_event_hdr_t  *msg_ptr;
    301            uint16 length = 0;
    302            uint16 rxBufLen  = Hal_UART_RxBufLen(MT_UART_DEFAULT_PORT);
    303          
    304            /*
    305               If maxZAppBufferLength is 0 or larger than current length
    306               the entire length of the current buffer is returned.
    307            */
    308            if ((MT_UartMaxZAppBufLen != 0) && (MT_UartMaxZAppBufLen <= rxBufLen))
    309            {
    310              length = MT_UartMaxZAppBufLen;
    311            }
    312            else
    313            {
    314              length = rxBufLen;
    315            }
    316          
    317            /* Verify events */
    318            if (event == HAL_UART_TX_FULL)
    319            {
    320              // Do something when TX if full
    321              return;
    322            }
    323          
    324            if (event & ( HAL_UART_RX_FULL | HAL_UART_RX_ABOUT_FULL | HAL_UART_RX_TIMEOUT))
    325            {
    326              if ( App_TaskID )
    327              {
    328                /*
    329                   If Application is ready to receive and there is something
    330                   in the Rx buffer then send it up
    331                */
    332                if ((MT_UartZAppRxStatus == MT_UART_ZAPP_RX_READY ) && (length != 0))
    333                {
    334                  /* Disable App flow control until it processes the current data */
    335                   MT_UartAppFlowControl (MT_UART_ZAPP_RX_NOT_READY);
    336          
    337                  /* 2 more bytes are added, 1 for CMD type, other for length */
    338                  msg_ptr = (osal_event_hdr_t *)osal_msg_allocate( length + sizeof(osal_event_hdr_t) );
    339                  if ( msg_ptr )
    340                  {
    341                    msg_ptr->event = SPI_INCOMING_ZAPP_DATA;
    342                    msg_ptr->status = length;
    343          
    344                    /* Read the data of Rx buffer */
    345                    HalUARTRead( MT_UART_DEFAULT_PORT, (uint8 *)(msg_ptr + 1), length );
    346          
    347                    /* Send the raw data to application...or where ever */
    348                    osal_msg_send( App_TaskID, (uint8 *)msg_ptr );
    349                  }
    350                }
    351              }
    352            }
