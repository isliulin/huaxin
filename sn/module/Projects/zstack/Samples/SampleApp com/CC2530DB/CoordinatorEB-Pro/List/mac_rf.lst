    398          static uint8 basicRfBuildHeader(uint8* buffer, uint16 destAddr, uint8 payloadLength)
    399          {
    400              basicRfPktHdr_t *pHdr;
    401              uint16 fcf;
    402          
    403              pHdr= (basicRfPktHdr_t*)buffer;
    404          
    405              // Populate packet header
    406          #ifndef SEND_OBEY_ZSTACK
    407              pHdr->packetLength = payloadLength + BASIC_RF_PACKET_OVERHEAD_SIZE;
    408          #endif
    409              //pHdr->frameControlField = pConfig->ackRequest ? BASIC_RF_FCF_ACK : BASIC_RF_FCF_NOACK;
    410             // fcf= pConfig->ackRequest ? BASIC_RF_FCF_ACK : BASIC_RF_FCF_NOACK;
    411              fcf = pConfig->frameHead;
    412              pHdr->fcf0 = LO_UINT16(fcf);
    413              pHdr->fcf1 = HI_UINT16(fcf);
    414              pHdr->seqNumber= txState.txSeqNumber;
    415              pHdr->panId= pConfig->panId;
    416              pHdr->destAddr= destAddr;
    417              pHdr->srcAddr= pConfig->myAddr;
    418          
    419          #ifdef SECURITY_CCM
    420          
    421              // Add security to FCF, length and security header
    422              pHdr->fcf0 |= BASIC_RF_SEC_ENABLED_FCF_BM_L;
    423              pHdr->packetLength += PKT_LEN_MIC;
    424              pHdr->packetLength += BASIC_RF_AUX_HDR_LENGTH;
    425          
    426              pHdr->securityControl= SECURITY_CONTROL;
    427              pHdr->frameCounter[0]=   LO_UINT16(LO_UINT32(txState.frameCounter));
    428              pHdr->frameCounter[1]=   HI_UINT16(LO_UINT32(txState.frameCounter));
    429              pHdr->frameCounter[2]=   LO_UINT16(HI_UINT32(txState.frameCounter));
    430              pHdr->frameCounter[3]=   HI_UINT16(HI_UINT32(txState.frameCounter));
    431          	
    432          #endif
    433          
    434              // Make sure bytefields are network byte order
    435              //UINT16_HTON(pHdr->panId);
    436             // UINT16_HTON(pHdr->destAddr);
    437              //UINT16_HTON(pHdr->srcAddr);
    438          #ifdef SEND_OBEY_ZSTACK
